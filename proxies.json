{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "web root": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/"
      },
      "backendUri": "https://mattmaguranyus.blob.core.windows.net/web/index.html"
    },
    "web assets": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/assets/{*file}"
      },
      "backendUri": "https://mattmaguranyus.blob.core.windows.net/web/assets/{file}"
    },
    "web static": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/{file}.{extension}"
      },
      "backendUri": "https://mattmaguranyus.blob.core.windows.net/web/{file}.{extension}"
    }
  }
}